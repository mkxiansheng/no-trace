{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".idea"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
  },
  "watch": [
    "config/",
    "models/",
    "routes/",
    "views/",
    "index.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js html css less json"
}